{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "camelcase": 0,
    "block-scoped-var": 0,
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "guard-for-in": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-nested-ternary": 0,
    "no-restricted-syntax": 0,
    "spaced-comment": 0,
    "no-param-reassign": 1,
    "no-plusplus": 1,
    "no-mixed-operators": 1,
    "arrow-parens": 1,
    "no-unused-expressions": 1,
    "no-confusing-arrow": 1,
    "max-len": 1,
    "object-property-newline": 1,
    "no-return-assign": 1,
    "arrow-body-style": 1,
    "consistent-return": 1,
    "import/no-named-as-default": 1,
    "valid-typeof": 1,
    "no-underscore-dangle": 1,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "import/imports-first": 0
  },
  "parser": "babel-eslint",
  "plugins": [
    "jest"
  ],
  "globals": {
    "__THEME_ID__": false,
    "__DEBUG__": false,
    "__DEV__": false,
    "__PROD__": false,
    "__ENV__": false,
    "__API_URL__": false,
    "_gaq": true,
    "expect": false,
    "jest": false,
    "render": false,
    "ANY": false,
    "__EXTERNAL_CONFIG__": false,
  }
}